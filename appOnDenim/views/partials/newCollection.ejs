<section class="carrusel-newColection flex-colum">
        <div class="contenedor-newColection">
                <div>
                        <h2 class="titulo-newColection">New Collection</h2>
                </div>
        <div class="contenedor-productos contenedor-productosNew  flex-colum">
        <!-- producto NewCollection -->
        <% products.forEach((producto, index) => { %>
                <% if (producto.newCollection && index <= 3) { %>
                        <div class="cajas-productosPersonalizados cajas-productosPersonalizados-new  flex-colum">
                                <a href="/producto/<%= producto.id %>">
                                        <div class="img-producto img-productoNew">
                                                <img src="/images/productos/<%= producto.image %>" alt="Producto 1">
                                        </div> 
                                </a>
                        </div>
                <% } %>
        <%  })%>

        </div>
        <i class="fas fa-greater-than arrow-left arrow-leftNew"></i>
        <i class="fas fa-greater-than arrow-right arrow-rightNew"></i>  
        </div>   
